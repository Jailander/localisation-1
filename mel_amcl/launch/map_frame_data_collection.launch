<?xml version="1.0" encoding="ISO-8859-15"?>

<launch>

  <arg name="bag_prefix" default="data"/>

  <!-- bagging data -->
  <node pkg="rosbag" type="record" name="rosbag_record_standard_data" respawn="true" output="screen" 
  args="-o $(find mel_amcl)/bag_data/map_frame/$(arg bag_prefix) /tf_static /odometry/base_raw /twist_mux/cmd_vel /nmea/main_GGA /yaw /gps/fix /scan /imu/data" />

</launch>
